{
  "$schema": "vscode://schemas/icon-theme",
  "iconDefinitions": {
    "pumpkin": {
      "iconPath": "../assets/pumpkin.svg"
    },
    "skull": {
      "iconPath": "../assets/skull.svg"
    },
    "ghost": {
      "iconPath": "../assets/ghost.svg"
    },
    "scroll": {
      "iconPath": "../assets/scroll.svg"
    },
    "coffin": {
      "iconPath": "../assets/coffin.svg"
    },
    "coffin-open": {
      "iconPath": "../assets/coffin-open.svg"
    }
  },
  "fileExtensions": {
    "js": "pumpkin",
    "ts": "skull",
    "json": "ghost",
    "md": "scroll"
  },
  "folder": "coffin",
  "folderExpanded": "coffin-open"
}
