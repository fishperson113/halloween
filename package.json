{
  "name": "halloween-kirotheme",
  "displayName": "Kiroween",
  "description": "Halloween VSCode theme made for Kiroween Hackathon",
  "version": "0.0.1",
  "publisher": "fishperson113",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Themes",
    "Other"
  ],
  "contributes": {
    "themes": [
      {
        "label": "KiroTheme",
        "uiTheme": "vs-dark",
        "path": "./themes/KiroTheme-color-theme.json"
      }
    ],
    "productIconThemes": [
      {
        "id": "kiroween-icons",
        "label": "Kiroween Icons",
        "path": "./themes/product-icon-theme.json"
      }
    ],
    "configuration": {
      "title": "Kiroween",
      "properties": {
        "kiroween.background.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable or disable the Halloween background pattern in the editor"
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fishperson113/halloween.git"
  }
}
